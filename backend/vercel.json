{
  "version":2,
  "bulds":[
    {"src": "*.js", "use" : "@vercel/node"}
  ],
   "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://imagine-in.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        }
      ]
"routes":[
  {
    "src": "/(.*)",
    "dest":"/"

  }
]
}
